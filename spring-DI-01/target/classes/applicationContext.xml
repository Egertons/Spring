<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="stu" class="com.zhang.pojo.Student">
        <!-- 第一种，普通值注入。-->
        <property name="name" value="zhanglin"/>
        <!-- 第二种，bean注入。-->
        <property name="address" ref="address"/>
        <!-- 第三种，SET注入 -->
        <property name="books">
            <array>
                <value>西游记</value>
                <value>水浒传</value>
                <value>三国演义</value>
                <value>红楼梦</value>
            </array>
        </property>
        <!-- 第四种，LIST注入 -->
        <property name="hobbies">
            <list>
                <value>听歌</value>
                <value>写JAVA</value>
                <value>写Python</value>
                <value>读书</value>
            </list>
        </property>
        <!--Map-->
        <property name="card">
            <map>
                <entry key="身份证" value="220202200006154213"/>
                <entry key="book_id" value="299787676"/>
            </map>
        </property>
        <!--Set-->
        <property name="games">
            <set>
                <value>LOL</value>
                <value>COD</value>
                <value>Battlefield_5</value>
            </set>
        </property>
        <!--null-->
<!--        <property name="wife" value=""/>-->
        <property name="wife">
<!--            <null></null>-->
            <null/>
        </property>
        <!-- Properties -->
        <property name="info">
            <props>
                <prop key="administrator">administrator@example.org</prop>
                <prop key="support">support@example.org</prop>
            </props>
        </property>
    </bean>
    <bean id="address" class="com.zhang.pojo.Address">
        <property name="address" value="JiLin"></property>
    </bean>
    <import resource="userbeans.xml"/>
</beans>